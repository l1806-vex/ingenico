<!DOCTYPE mbeans-descriptors PUBLIC
 "-//Apache Software Foundation//DTD Model MBeans Configuration File"
 "http://jakarta.apache.org/commons/dtds/mbeans-descriptors.dtd">

<!-- Descriptions of JMX MBeans for Catalina -->

<mbeans-descriptors>

    <mbean name="SessionCounters" description="All sessions counters"
        type="com.ingenico.oegw.stat.TotalStatistics">
        <attribute name="activeConnections"
            description="Concurrent connections" writeable="false"
            displayName="Concurrent connections" type="int" />
        <attribute name="avgSessionTime"
            description="Average session duration" writeable="false"
            displayName="Concurrent connections" type="long" />
        <attribute name="avgReceivedDatas"
            description="Average amount of datas transferred from terminal"
            writeable="false" type="long"/>
        <attribute name="avgSentDatas"
            description="Average amount of datas transferred to terminal"
            writeable="false" type="long"/>
        <attribute name="maxSimultaneouslyClients"
            description="Maximun concurrent connections reached." writeable="false"
            displayName="Max concurrent connections" type="int" />
        <attribute name="maxConcurrentConnectionsThreshold"
            description="Maximun concurrent connections threshold" writeable="true"
            displayName="Max concurrent connections threshold" type="int" />
    
   
        <attribute name="avgIterationTime"
            description="Average iteration duration" writeable="false"
            displayName="Average iteration duration" type="long" />
            
        <attribute name="maxIterationTime"
            description="Maximun iteration duration reached" 
            writeable="false"
            displayName="Maximun iteration duration" type="long" />
        <attribute name="maxIterationTimeThreshold"
            description="Maximun iteration duration threshold" 
            writeable="true"
            displayName="Iteration threshold" type="long" />
            
        <attribute name="avgOegwTime"
            description="Average oegw-terminal interactions duration for a session" writeable="false"
            displayName="Average oegw-terminal interactions duration" type="long" />
            
        <attribute name="avgWebTime"
            description="Average oegw-web interactions duration for a session" writeable="false"
            displayName="Average oegw-web interactions duration" type="long" />
            
        <attribute name="avgDbTime"
            description="Average oegw-db interactions duration for a session" writeable="false"
            displayName="Average oegw-db interactions duration" type="long" />
            
        <attribute name="maxOegwTime"
            description="Maximun oegw-terminal interactions duration reached for a session" 
            writeable="false"
            displayName="Maximun oegw-terminal interactions" type="long" />
            
        <attribute name="maxOegwTimeThreshold"
            description="Maximun oegw-terminal interactions duration threshold" 
            writeable="true"
            displayName="oegw-terminal interactions threshold" type="long" />
            
        <attribute name="maxWebTime"
            description="Maximun oegw-web interactions duration reached for a session" 
            writeable="false"
            displayName="Maximun oegw-web interactions" type="long" />
        <attribute name="maxWebTimeThreshold"
            description="Maximun oegw-web interactions duration threshold" 
            writeable="true"
            displayName="oegw-web interactions threshold" type="long" />
            
        <attribute name="maxDbTime"
            description="Maximun oegw-db interactions duration reached for a session" 
            writeable="false"
            displayName="Maximun oegw-db interactions" type="long" />
        <attribute name="maxDbTimeThreshold"
            description="Maximun oegw-db interactions duration threshold" 
            writeable="true"
            displayName="oegw-db interactions threshold" type="long" />
    </mbean>

</mbeans-descriptors>


